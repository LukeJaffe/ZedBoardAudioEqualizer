#include "fprof.h"

void fprof_init(fprof_t* this)
{
    int i;
    //for (i = 0; i < NUM_BANDS; i++)
    //  this->bands[i] = 4;

    this->bands[0] = 4;
    this->bands[1] = 3;
    this->bands[2] = 5;
    this->bands[3] = 4;
    this->bands[4] = 6;
    this->bands[5] = 5;
    this->bands[6] = 7;
    this->bands[7] = 6;

    this->selected = 3;

    // initialize frequency scalars
    for (i = 0; i < SCALING_SIZE; i++)
    	frequency_scalars[i] = 1.0;
}

			if (i < 8)
			{
				//frequency_scalars[j] = (float)this->bands[i]/(float)MID_BAND_VAL;
			}
			else
			{
				//frequency_scalars[j] = 1.0;
			}
    	}
    }

    printf("Frequencies assigned\n");
}

